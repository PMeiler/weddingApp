<% include partials/header %>

<% console.log(guest); %>
<% var a = 1;%>


<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Name</th>
      <th scope="col">E-Mail</th>
      <th scope="col">Number</th>
      <th scope="col">Guests</th>
    </tr>
  </thead>
  <tbody>
      
      <% guest.forEach(function(c){ %>
    <tr>
      <th scope="row"><%= a %></th>
      <% a+=1; %>
      <td><%= c.name %></td>
      <td><%= c.email %></td>
      <td><%= c.number %></td>
      <td><%= c.guests %></td>
    </tr>
    
   <%   }); %>
  </tbody>
</table>


<% include partials/footer %>